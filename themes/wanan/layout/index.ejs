  <script src="https://cdn.bootcss.com/jquery.perfect-scrollbar/1.4.0/perfect-scrollbar.min.js"></script>
  <script>
    hljs.initHighlightingOnLoad();
    hljs.configure({
      useBR: false
    });
    $('div.code').each(function (i, block) {
      hljs.highlightBlock(block);
    });
